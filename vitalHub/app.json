{
	"expo": {
		"name": "vitalhub",
		"slug": "vitalhub",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./src/assets/logo.png",
		"userInterfaceStyle": "light",
		"splash": {
			"image": "./src/assets/splash.png",
			"resizeMode": "contain"
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true,
			"config": {
				"userInterfaceStyle": "light",
				"statusBar": {
					"style": "light",
					"backgroundColor": "transparent",
					"translucent": true
				}
			}
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./src/assets/Splash.png",
				"backgroundColor": "#ffffff"
			}
		},
		"web": {
			"favicon": "./src/assets/favicon.png"
		}
	}
}
